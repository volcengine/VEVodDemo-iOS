  # 添加 TTSDK
  pod 'TTSDK', '1.31.1.4-premium', :subspecs => ['Player']
  
  # 添加 Demo 运行需要依赖的三方库
  pod 'Masonry'
  pod 'SDWebImage'
  pod 'MBProgressHUD', '~> 1.2.0'
  pod 'Reachability'
  pod 'MJRefresh'
  
end

